{
  "log": {
    "disabled": false,
    "level": "info",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "tag": "dns_block",
        "address": "rcode://success"
      },
      {
        "tag": "dns_direct",
        "address": [
          "h3://dns.alidns.com/dns-query",
          "https://doh.pub/dns-query"
        ],
        "detour": "DIRECT"
      },
      {
        "tag": "dns_proxy",
        "address": [
          "h3://dns.google/dns-query",
          "h3://cloudflare-dns.com/dns-query"
        ]
      },
      {
        "tag": "dns_fakeip",
        "address": "fakeip"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "server": "dns_direct"
      },
      {
        "clash_mode": "Direct",
        "server": "dns_direct"
      },
      {
        "clash_mode": "Global",
        "server": "dns_proxy"
      },
      {
        "rule_set": [
          "win-spy"
        ],
        "server": "dns_block",
        "disable_cache": true,
        "rewrite_ttl": 0
      },
      {
        "rule_set": [
          "win-update",
          "microsoft-cn",
          "apple-cn",
          "cn",
          "private",
          "direct-self",
          "ntpservice"
        ],
        "query_type": [
          "A",
          "AAAA"
        ],
        "server": "dns_direct"
      },
      {
        "rule_set": [
          "proxy",
          "google-cn",
          "proxy-self",
          "telegram",
          "ai"
        ],
        "query_type": [
          "A",
          "AAAA"
        ],
        "server": "dns_proxy",
        "rewrite_ttl": 0
      },
      {
        "fallback_rules": [
          {
            "rule_set": [
              "cn-ip"
            ],
            "server": "dns_direct"
          },
          {
            "match_all": true,
            "server": "dns_proxy",
            "rewrite_ttl": 0
          }
        ],
        "allow_fallthrough": true,
        "server": "dns_proxy"
      }
    ],
    "final": "dns_direct",
    "strategy": "prefer_ipv4",
    "independent_cache": true,
    "reverse_mapping": true,
    "disable_cache": false,
    "disable_expire": false,
    "fakeip": {
      "enabled": true,
      "inet4_range": "198.18.0.0/15",
      "inet6_range": "fc00::/18"
    },
    "hosts": {
      "miwifi.com": [
        "192.168.31.1"
      ],
      "doh.pub": [
        "1.12.12.12",
        "120.53.53.53",
        "2402:4e00::"
      ],
      "dns.alidns.com": [
        "223.5.5.5",
        "223.6.6.6",
        "2400:3200::1",
        "2400:3200:baba::1"
      ],
      "dns.google": [
        "8.8.8.8",
        "8.8.4.4",
        "2001:4860:4860::8888",
